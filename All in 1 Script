local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

local Window = OrionLib:MakeWindow({
    Name = "The Classic Hub",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "TheClassicFiles"
})

local HubTab = Window:MakeTab({
    Name = "The Hub",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local function teleportToPlace(placeId)
    if game.PlaceId == placeId then
        OrionLib:MakeNotification({
            Name = "The Hub",
            Content = "You are already in the game.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    else
        game:GetService("TeleportService"):Teleport(placeId, game.Players.LocalPlayer)
        wait(2)
    end
end

local Section1 = HubTab:AddSection({
    Name = "A Dusty Trip [CLASSIC‚ùóÔ∏è]"
})

Section1:AddButton({
    Name = "A Dusty Trip (Tix Farm & Token Farm)",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ToraScript/Script/main/ClassicAdustyTrip", true))()
    end
})

Section1:AddButton({
    Name = "Teleport to A Dusty Trip [CLASSIC‚ùóÔ∏è]",
    Callback = function()
        teleportToPlace(16389395869)
    end
})

