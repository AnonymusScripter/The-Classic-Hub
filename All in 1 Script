-- hey skid.

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("The Classic Hub", "BloodTheme")

local function teleportToPlace(placeId)
    if game.PlaceId == placeId then
        Library:Notify("The Hub", "You are already in the game.", "rbxassetid://4483345998", 5)
    else
        game:GetService("TeleportService"):Teleport(placeId, game.Players.LocalPlayer)
        wait(2)
    end
end

local function addButton(section, name, info, callback)
    local button = section:NewButton(name, info, callback)
    return button
end

local HubTab = Window:NewTab("The Hub")

local Section3 = HubTab:NewSection("Adopt Me!")

addButton(Section3, "Adopt Me! (Tix Farm & Token Farm)", "Loads the Adopt Me! script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ToraScript/Script/main/ClassicAdoptMe", true))()
end)

addButton(Section3, "Teleport to Adopt Me!", "Teleports you to Adopt Me!", function()
    teleportToPlace(920587237)
end)

